{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": [
    "import",
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "import/no-extraneous-dependencies": "warn",
    "no-use-before-define": ["error", { "functions": false }],
    "react/jsx-filename-extension": ["warn", { "extensions": [".js"] }]
  }
}